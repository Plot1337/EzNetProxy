<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <DebugType>embedded</DebugType>
	  
    <ProduceReferenceAssembly>False</ProduceReferenceAssembly>
    <GenerateDocumentationFile>False</GenerateDocumentationFile>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	  
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/Plot1337/EzNetProxy</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Plot1337/EzNetProxy</RepositoryUrl>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Authors>Plot1337</Authors>
    <Version>1.1.0.0</Version>
    <PackageId>EzNetProxy</PackageId>
    <Description>A library to simplify proxy usage in .NET</Description>
    <PackageTags>csharp, dotnet, proxy, socks, socks-proxy</PackageTags>
	  
    <EnforceCodeStyleInBuild>False</EnforceCodeStyleInBuild>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
